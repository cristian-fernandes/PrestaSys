@model Unisul.PrestaSys.Web.Models.Usuarios.UsuarioViewModel

@{
    ViewData["Title"] = "Detalhes do Usuário";
}

<h1>Detalhes do Usuário</h1>

<hr />

<div>
    <dl class="row">
        <dt class="col-sm-2">
            @Html.DisplayNameFor(model => model.Nome)
        </dt>
        <dd class="col-sm-10">
            @Html.DisplayFor(model => model.Nome)
        </dd>
        <dt class="col-sm-2">
            @Html.DisplayNameFor(model => model.Sobrenome)
        </dt>
        <dd class="col-sm-10">
            @Html.DisplayFor(model => model.Sobrenome)
        </dd>
        <dt class="col-sm-2">
            @Html.DisplayNameFor(model => model.Email)
        </dt>
        <dd class="col-sm-10">
            @Html.DisplayFor(model => model.Email)
        </dd>
        <dt class="col-sm-2">
            @Html.DisplayNameFor(model => model.Senha)
        </dt>
        <dd class="col-sm-10">
            @Html.DisplayFor(model => model.Senha)
        </dd>
        <dt class="col-sm-2">
            @Html.DisplayNameFor(model => model.FlagGerente)
        </dt>
        <dd class="col-sm-10">
            @Html.DisplayFor(model => model.FlagGerente)
        </dd>
        <dt class="col-sm-2">
            @Html.DisplayNameFor(model => model.FlagGerenteFinanceiro)
        </dt>
        <dd class="col-sm-10">
            @Html.DisplayFor(model => model.FlagGerenteFinanceiro)
        </dd>
        <dt class="col-sm-2">
            @Html.DisplayNameFor(model => model.Gerente)
        </dt>
        <dd class="col-sm-10">
            @Html.DisplayFor(modelItem => modelItem.Gerente.Nome) @Html.DisplayFor(modelItem => modelItem.Gerente.Sobrenome)
        </dd>
        <dt class="col-sm-2">
            @Html.DisplayNameFor(model => model.GerenteFinanceiro)
        </dt>
        <dd class="col-sm-10">
            @Html.DisplayFor(modelItem => modelItem.GerenteFinanceiro.Nome) @Html.DisplayFor(modelItem => modelItem.GerenteFinanceiro.Sobrenome)
        </dd>
    </dl>
</div>
<div>
    <a asp-action="Edit" asp-route-id="@Model.Id">Editar</a> |
    <a asp-action="Index">Voltar para a Lista</a>
